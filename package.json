    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-loader": "0.13.0",
    "react-dev-utils": "^0.2.1",
    "react-hot-loader": "^3.0.0-beta.5",
    "recursive-readdir": "2.0.0",
    "rimraf": "2.5.4",
    "sass-lint": "^1.9.1",
    "sass-loader": "^4.0.2",
    "strip-ansi": "3.0.1",
    "style-loader": "github:bendytree/style-loader",
    "svg-sprite-loader": "0.0.28",
